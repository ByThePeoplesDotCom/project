ter"}], "updated_at": "2012-11-02 06:44:24", "action_dates": {"passed_upper": null, "passed_lower": null, "last": "2012-04-11 00:00:00", "signed": null, "first": "2012-04-03 00:00:00"}, "level": "state", "country": "us", "created_at": "2012-04-03 19:41:38", "versions": [{"url": "http://www.leg.wa.gov/pub/billinfo/2011-12/Htm/Bills/House Bills/2827.htm", "mimetype": "text/html", "doc_id": "WAD00004063", "name": "HB 2827"}], "alternate_titles": [], "chamber": "lower", "bill_id": "HB 2827"}